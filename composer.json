{
    "name": "oro/cron-bundle",
    "type": "symfony-bundle",
    "description": "BAP Cron Bundle",
    "keywords": ["BAP", "cron"],
    "homepage": "https://github.com/laboro/CronBundle",
    "license": "MIT",
    "require": {
        "php": ">=5.3.3",
        "symfony/symfony": "2.3.*",
        "jms/job-queue-bundle": "dev-master",
        "mtdowling/cron-expression": "1.0.*"
    },
    "autoload": {
        "psr-0": { "Oro\\Bundle\\CronBundle": "" }
    },
    "target-dir": "Oro/Bundle/CronBundle",
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}